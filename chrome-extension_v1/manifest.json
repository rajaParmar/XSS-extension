{
  // Required
  "manifest_version": 2,
  "name": "STOP XSS",
  "version": "1.0",

  // Recommended
  "description": "Saves you form XSS",

  "background" : {
  	"scripts" : ["background.js"]
  },
  "content_scripts" : [
		{
			"matches" : ["http://*/*","https://*/*"],
			"js" : ["safety_inject.js"]
		}

	],

  "permissions" : ["<all_urls>","webRequest","webRequestBlocking"]


}